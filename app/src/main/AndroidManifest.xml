<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.feedwoofs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.feedwoofs.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.RegisterActivity"
            android:parentActivityName="com.example.feedwoofs.MainActivity" >

        </activity>
        <activity
            android:name="com.example.feedwoofs.HomeActivity2"
            android:parentActivityName="com.example.feedwoofs.MainActivity" >

        </activity>

        <activity
            android:name="com.example.feedwoofs.ListAhliPakanActivity"
            android:parentActivityName="com.example.feedwoofs.HomeActivity2" >

        </activity>

        <activity
            android:name="com.example.feedwoofs.ChatActivity"
            android:parentActivityName="com.example.feedwoofs.ListAhliPakanActivity" >

        </activity>

        <activity
            android:theme="@style/AppTheme3"
            android:name="com.example.feedwoofs.ProfileActivity"
            android:parentActivityName="com.example.feedwoofs.ListAhliPakanActivity" >

        </activity>

        <activity
            android:name="com.example.feedwoofs.ProfileUserActivity">

        </activity>

        <activity
            android:name="com.example.feedwoofs.PesananActivity"
            android:parentActivityName="com.example.feedwoofs.HomeActivity2">

        </activity>

        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.OrderActivity"
            android:parentActivityName=".ListAhliPakanActivity" >

        </activity>

        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.DataDiriUserActivity"
            android:parentActivityName=".ProfileUserActivity" >

        </activity>

        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.UbahDataDiriActivity"
            android:parentActivityName=".DataDiriUserActivity" >

        </activity>


        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.DetailPermintaanActivity"
            android:parentActivityName=".PesananActivity" >

        </activity>

        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.PembayaranActivity"
            android:parentActivityName=".PesananActivity" >

        </activity>

        <activity
            android:name="com.example.feedwoofs.SelesaiActivity"
            android:parentActivityName=".PesananActivity" >

        </activity>

        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.feedwoofs.PesananSudahDibayar"
            android:parentActivityName=".ListAhliPakanActivity" >

        </activity>

        <activity android:name=".SplashPelatihan" android:noHistory="true" />
        <activity android:name=".SplashPelatihanSelesai" android:noHistory="true" />
        <activity android:name=".SplashProfileBerhasil"  android:noHistory="true"/>
    </application>

</manifest>